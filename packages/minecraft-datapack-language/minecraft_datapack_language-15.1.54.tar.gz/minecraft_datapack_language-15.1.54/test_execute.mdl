pack "test_execute" "Test execute statement spacing" 82;

namespace "test";

function "main" {
    // Test MDL execute statement syntax
    execute as @a function "test:hello";
    
    // Test raw execute command for comparison
    $!raw
    execute as @a run function test:hello
    raw!$
}

function "hello" {
    say Hello from execute test!;
}
