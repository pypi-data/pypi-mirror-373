// Raw Commands Example
pack "raw_commands" "Example showing raw command usage" 82;

namespace "raw_commands";

function "main" {
    say Starting raw command demo...;
    
    $!raw
    # This is a raw command block
    # These commands are inserted directly without processing
    effect give @a minecraft:night_vision 10 1 true
    effect give @a minecraft:jump_boost 10 2 true
    raw!$
    
    say Raw commands executed!;
}

function "custom_commands" {
    $!raw
    # Custom Minecraft commands
    gamemode creative @a
    weather clear
    time set day
    raw!$
    
    say Custom commands executed!;
}

on_load "raw_commands:main";
