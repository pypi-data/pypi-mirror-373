        "bkpi_payload_data_type": {
            "LoanDuration": "int",
            "LoanAmount": "int",
            "InstallmentPercent": "int",
            "AcceptedPercent": "float",
            "Accepted": "int",
            "AmountGranted": "int"
        },
        "business_application": {
            "name": "TBD",
            "description": "Credit Risk Business Application",
            "payload_fields": [
                {
                    "name": "LoanDuration",
                    "type": "number",
                    "description": "Duration of the loan"
                },
                {
                    "name": "LoanPurpose",
                    "type": "string",
                    "description": "Purpose of the loan"
                },
                {
                    "name": "LoanAmount",
                    "type": "number",
