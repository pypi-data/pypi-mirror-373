    {% extends "collection.json.j2" %}
    {% block id %}
      "@id": "./",
      "license": {"@id": "https://creativecommons.org/licenses/by/4.0/"},
      "datePublished": "{{ manifest.timestamps[0] }}",
    {% endblock %}
    {% block hasMember %}
      {% for person in manifest.members  %}{"@id": "{{ person.first_name }} {{ person.last_name }}"}{% if not loop.last %},{% endif %}{% endfor %}
    {% endblock %}
    {% block hasPart %}
      {% for study in manifest.studies %}{% if study.id in collection.children %}{"@id": "./{{ study.name }}"},{% endif %}{% for assay in manifest.assays if assay.id in study.children %}{"@id": "./{{ study.name }}/{{ assay.name }}"},{% for image in manifest.images if image.id in assay.images %}{% for study in manifest.studies if study.id in assay.parents %}{"@id": "./{{ study.name }}/{{ assay.name }}/{{ image.name }}"},{% endfor %}{% endfor %}{% endfor %}{% endfor %}{"@id": "end_item_parts"}
    {% endblock %}
    {% block keywords %}
      {% for annotation in manifest.quay_annotations if annotation.ann_type == "tag_annotation" and annotation.id in collection.quay_annotations %}{"item": "{{ annotation.value }}"}{% if not loop.last %},{% endif %}{% endfor %}
    {% endblock %}
    {% block subjectOf %}
      {% for annotation in collection.quay_annotations if annotation.ann_type != "tag_annotation" %}{"@id": "{{ annotation }}"}{% if not loop.last %},{% endif %}{% endfor %}
    {% endblock %}
