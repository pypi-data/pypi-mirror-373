{
  "id": "docker",
  "ror_ids": [],
  "host": "omero-server",
  "route": [
    "mesoUserResc",
    "mesoCoopResc",
    "mesoOmeroUser",
    "mesoOmero",
    "mesoRocrate",
    "mesoCoopResc"
  ],
  "stores": [
    {
      "id": "mesoOmeroUser",
      "scheme": "omero",
      "host": "omero-server",
      "resc": null,
      "data_roots": [],
      "is_isa": false,
      "post_url": "http://omero-server:8888"
    },
    {
      "id": "mesoCoopResc",
      "scheme": "irods",
      "host": "omero-server",
      "resc": "mesoCoopResc",
      "data_roots": ["irods:///tempZone/home", "file:///mnt/coop/"],
      "is_isa": true,
      "post_url": "http://omero-server:8888"
    },
    {
      "id": "mesoUserResc",
      "scheme": "irods",
      "host": "omero-server",
      "resc": "demoResc",
      "data_roots": [
        "irods:///tempZone/home/{{ manifest.manager }}",
        "file:///mnt/user/home/{{ manifest.manager }}"
      ],
      "is_isa": false,
      "post_url": "http://omero-server:8888"
    },
    {
      "id": "mesoRocrate",
      "scheme": "file",
      "host": "omero-server",
      "resc": null,
      "data_roots": ["file:///mnt/coop/"],
      "is_isa": true,
      "post_url": "http://omero-server:8888"
    },
    {
      "id": "mesoOmero",
      "scheme": "omero",
      "host": "omero-server",
      "resc": null,
      "data_roots": ["omero:///mnt/SHARE/", "file:///mnt/SHARE/"],
      "is_isa": true,
      "post_url": "http://omero-server:8888"
    }
  ]
}
