    {
    {% block id %}
    {% endblock %}
    {% block head %}
      "name": "{{ collection.name }}",
      "description": "{{ collection.description | replace('\n', ' ') }}",
    {% if collection.creation_date %}
      "dateCreated": "{{ collection.creation_date }}",
    {% endif %}
      "author": "{{ collection.owner }}",
    {% endblock %}
    {% if collection.children %}
      "hasPart": [{% block hasPart %}{% endblock %}],
    {% endif %}
    {% if manifest.members and collection == investigation %}
      "hasMember": [{% block hasMember %}{% endblock %}],
    {% endif %}
    {% if manifest.quay_annotations and collection.quay_annotations %}
      "keywords": [{% block keywords %}{% endblock %}],
    {% endif %}
    {% if collection.quay_annotations %}
      "subjectOf": [{% block subjectOf %}{% endblock %}],
    {% endif %}
      "@type": "Dataset"
    }
