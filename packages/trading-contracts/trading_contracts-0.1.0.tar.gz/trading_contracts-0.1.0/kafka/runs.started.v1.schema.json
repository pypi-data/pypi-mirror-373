{
  "$id": "kafka/runs.started.v1.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "runs.started@v1",
  "type": "object",
  "required": ["event_id","version","run_id","strategy_id","instance_id","status","ts"],
  "properties": {
    "event_id": {"type":"string"},
    "version": {"const":1},
    "run_id": {"type":"string"},
    "strategy_id": {"type":"string"},
    "instance_id": {"type":"string"},
    "status": {"enum":["STARTED","RUNNING","PAUSED","STOPPED"]},
    "config": {"type":"object"},
    "ts": {"type":"string", "format":"date-time"}
  },
  "additionalProperties": false
}
