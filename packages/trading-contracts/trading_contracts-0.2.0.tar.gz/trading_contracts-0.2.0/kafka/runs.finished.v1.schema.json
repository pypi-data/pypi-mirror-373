{
  "$id": "kafka/runs.finished.v1.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "runs.finished@v1",
  "type": "object",
  "required": ["event_id","version","run_id","ts"],
  "properties": {
    "event_id": {"type":"string"},
    "version": {"const":1},
    "run_id": {"type":"string"},
    "status": {"enum":["FINISHED","STOPPED"]},
    "result": {"type":"string"},
    "ts": {"type":"string","format":"date-time"}
  },
  "additionalProperties": false
}
