<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_product_packaging_level_form" model="ir.ui.view">
        <field name="name">product.packaging.level.form</field>
        <field name="model">product.packaging.level</field>
        <field
            name="inherit_id"
            ref="product_packaging_level.view_product_packaging_level_form"
        />
        <field name="arch" type="xml">
            <group name="main" position="after">
                <group name="shopfloor" string="Shopfloor">
                    <group name="collect_settings">
                        <field name="shopfloor_collect_length" />
                        <field name="shopfloor_collect_width" />
                        <field name="shopfloor_collect_height" />
                        <field name="shopfloor_collect_weight" />
                    </group>
                </group>
            </group>
        </field>
    </record>
</odoo>
