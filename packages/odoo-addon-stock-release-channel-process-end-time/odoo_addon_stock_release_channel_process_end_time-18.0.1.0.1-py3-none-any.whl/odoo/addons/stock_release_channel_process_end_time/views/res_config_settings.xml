<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form_stock" model="ir.ui.view">
        <field
            name="name"
        >stock_release_channel_end_time res.config.settings form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <setting id="annual_inventory_date" position="after">
                <setting
                    id="stock_release_use_channel_end_date"
                    groups='stock.group_stock_manager'
                    help='Allow to update scheduled date follow process end date/time on stock release channel.'
                >
                    <field name="stock_release_use_channel_end_date" />
                </setting>
            </setting>
        </field>
    </record>
</odoo>
