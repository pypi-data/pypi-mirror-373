{
  "parameter_defines": [
    {
      "name": "use_tls",
      "label": "Use https",
      "type": "bool",
      "require": true,
      "default": false
    },
    {
      "name": "host",
      "label": "Hostname",
      "type": "string",
      "require": true,
      "default": "192.168.1.10"
    },
    {
      "name": "port",
      "label": "Port number",
      "type": "number",
      "require": true,
			"min": 1,
			"max": 65535,
      "default": 80
    },
    {
      "name": "path",
      "label": "URL path",
      "type": "string",
      "require": true,
      "default": "/"
    },
    {
      "name": "method",
      "label": "HTTP method",
      "type": "string",
      "require": true,
      "default": "POST"
    }
  ]
}
