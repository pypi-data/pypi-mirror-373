[run]
source = src/meta_learning
branch = true
omit = 
    tests/*
    */tests/*
    demo_*.py
    setup.py
    */old_archive/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Meta-Learning Coverage Report

[xml]
output = coverage.xml