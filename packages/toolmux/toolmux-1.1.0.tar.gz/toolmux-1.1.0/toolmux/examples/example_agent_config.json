{
  "name": "toolmux-agent",
  "description": "AI Agent with ToolMux - 98.65% token efficiency",
  "mcpServers": {
    "toolmux": {
      "command": "python",
      "args": ["/path/to/toolmux.py"],
      "timeout": 30000
    }
  },
  "tools": ["*"],
  "resources": [
    "file://./AGENT_INSTRUCTIONS.md"
  ],
  "hooks": {
    "agentSpawn": [
      {
        "command": "./toolmux_hook.sh",
        "timeout_ms": 5000,
        "max_output_size": 2048
      }
    ]
  }
}
