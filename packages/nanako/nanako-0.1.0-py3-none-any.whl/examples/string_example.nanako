# 文字列リテラルの例

# 基本的な文字列
message = "Hello"
name = "World"

# 文字列の末尾追加
message[?] = 32  # スペース
name回、くり返す{
    message[?] = name[0]  # 'W'
    message[?] = name[1]  # 'o'  
    message[?] = name[2]  # 'r'
    message[?] = name[3]  # 'l'
    message[?] = name[4]  # 'd'
}

# 結果: message = "Hello World"の文字コード配列