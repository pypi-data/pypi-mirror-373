# SSH connection parameters
SSH_HOST=localhost
SSH_PORT=22
SSH_USERNAME=your_username
SSH_PASSWORD=your_password_for_password_auth
SSH_PRIVATE_KEY=your_private_key_content_for_key_auth

# Security configuration
SSH_ALLOWED_COMMANDS=ls,pwd,echo,cat
SSH_ALLOWED_PATHS=/home,/tmp
SSH_COMMANDS_BLACKLIST=rm,mv,dd,mkfs,fdisk,format
SSH_ARGUMENTS_BLACKLIST=-rf,-fr,--force
