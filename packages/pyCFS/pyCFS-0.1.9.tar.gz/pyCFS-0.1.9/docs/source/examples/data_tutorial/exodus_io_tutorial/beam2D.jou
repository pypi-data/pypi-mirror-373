Journal Off
reset
undo off


# Define mesh intervall
#{ELB = 12}
#{ELL = 25}

# Define beam size
#{b = 3}
#{l = 100}


# create plane
create surface rectangle width {l} height {b} zplane 

# mesh plane
curve 1  interval {ELL}
curve 1  scheme equal 

curve 2  interval {ELB}
curve 2  scheme equal 
mesh curve 1 2

surface 1  scheme map
mesh surface 1 

# define regions
block 1 add surface all
block 1 name "beam"
block 1 element type quad8

sideset 1 add curve all with x_coord={- l/2}
sideset 1 name "BC_fix"
sideset 2 add curve all with x_coord={l/2}
sideset 2 name "reference_Load"

sideset 3 add curve all with x_coord={l/2}
sideset 3 name "reference_Load_surface"

export mesh "beam2D.e"  overwrite
